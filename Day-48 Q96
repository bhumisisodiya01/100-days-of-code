Q96: Reverse each word in a sentence without changing the word order.

/*
Sample Test Cases:
Input 1:
I love coding
Output 1:
I evol gnidoc

*/
#include <stdio.h>

int main() {
    char str[200];
    int i, start, end, j;
    gets(str);
    i = 0;
    while(str[i] != '\0') {
        start = i;
        while(str[i] != ' ' && str[i] != '\0') i++;
        end = i - 1;
        for(j = end; j >= start; j--)
            printf("%c", str[j]);
        if(str[i] == ' ')
            printf(" ");
        if(str[i] != '\0')
            i++;
    }
    return 0;
}
